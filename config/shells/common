# shellcheck source=/dev/null
source "${XDG_CONFIG_HOME:-${HOME}/.config}/shells/paths"

# shellcheck source=/dev/null
source "${XDG_CONFIG_HOME:-${HOME}/.config}/shells/xdg_zealotry"

# Only load my own custom Ranger config; not the global config. This allows me
# to remove things from the official configuration and to override mappings.
export RANGER_LOAD_DEFAULT_RC=FALSE
